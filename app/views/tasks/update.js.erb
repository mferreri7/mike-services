document.querySelector("div[data-id='<%=@task.id%>']").classList.toggle("tachado");
if (document.querySelector("div[data-id='<%=@task.id%>']").classList.contains("tachado")){
  document.querySelector("div[data-id='<%=@task.id%>']").querySelector(".complete-task-btn").innerText = "Deshacer"
} else {
  document.querySelector("div[data-id='<%=@task.id%>']").querySelector(".complete-task-btn").innerText = "Completar"
}
